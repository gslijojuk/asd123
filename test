concommand.Add("ib_menu", menu) -- The command to open the menu. Because some players don't have the insert key and this has been requested a lot. Now I've added a few more keys that can open the menu so this is just a very last alternative
CreateClientConVar("ib_name", "www.IDIOTBOX.net | Get tapped!", true, false)
local namechangeTime = 0
local function Think()
	if ((input.IsKeyDown(KEY_INSERT) or input.IsKeyDown(KEY_F11) or input.IsKeyDown(KEY_HOME)) and not menuopen and not insertdown) then
		menuopen = true
		insertdown = true
		menu()
	elseif (not (input.IsKeyDown(KEY_INSERT) or input.IsKeyDown(KEY_F11) or input.IsKeyDown(KEY_HOME)) and not menuopen) then
		insertdown = false
	end
	if ((input.IsKeyDown(KEY_INSERT) or input.IsKeyDown(KEY_F11) or input.IsKeyDown(KEY_HOME)) and insertdown and menuopen) then
		insertdown2 = true
	else
		insertdown2 = false
	end
	
