	local w = h / 1.7;
	local hp = em.Health(v) * h / 100;
	local health = em.Health(v);
	local col = (Color(gInt("Settings", "Entities Wallhack Color", "Red"),gInt("Settings", "Entities Wallhack Color", "Green"),gInt("Settings", "Entities Wallhack Color", "Blue")));
