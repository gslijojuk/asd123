local function mfinder()
	for k, v in pairs( ents.FindByClass( "weapon_mu_knife" ) ) do
	local pos = em.GetPos(v) + Vector( 1,0,-6 );
	local pos2 = pos + Vector(0, 0, 70);
	local pos = vm.ToScreen(pos);
	local pos2 = vm.ToScreen(pos2);
