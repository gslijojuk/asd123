local function msgRGB(time, text) -- hehe
	if not windowopen then
		windowopen = true

		local window = vgui.Create("DFrame")
		window:SetPos(ScrW()/2.7, 0)
		window:SetSize(500,25)
		window:SlideDown(0.3)
		window:SetTitle("")
		window:ShowCloseButton(false)
		window:SetDraggable(false)
		window.Paint = function(s, w, h)
			surface.SetDrawColor(40,40,40,240)
			surface.DrawRect(0, 0, w, h)
			--draw.RoundedBox(8, 0, 0, w, h, Color(40, 40, 40, 240))

			draw.DrawText(text, "MenuFont", w/2, 6, HSVToColor(RealTime()*69%360,1,1) || Color(0,0,0), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER)
		end

		timer.Simple(time, function()
			if windowopen then
				window:SlideUp(0.3)
				timer.Simple(0.3, function()
					windowopen = false
					window:Remove()
				end)
			end
		end)
	end

	chat.PlaySound()
	print("\n"..text.."\n")
end
