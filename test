function idiot.PlayerCheck()
	for k,v in pairs(player.GetAll()) do
	if (idiot.IsMe(v) and not v.Confirmed) then
		timer.Create( "ChatPrint2", 11.3, 1, function() msgRGB(6.9, "The creator of IdiotBox, "..v:Nick()..", is on the server!") end )
